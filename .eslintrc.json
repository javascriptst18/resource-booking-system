{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import",
    "react"
  ],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}